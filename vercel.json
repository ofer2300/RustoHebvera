{
    "version": 2,
    "builds": [
        {
            "src": "static/**",
            "use": "@vercel/static"
        },
        {
            "src": "target/release/rustohebru",
            "use": "@vercel/rust"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/api/(.*)",
            "dest": "/target/release/rustohebru"
        }
    ]
} 